<?xml version="1.0" encoding="UTF8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	metadata-complete="false" version="3.0">

	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>

	<listener>
		<listener-class>com.weizhu.webapp.mobile.GuiceConfigListener</listener-class>
	</listener>

	<filter>
		<filter-name>GuiceFilter</filter-name>
		<filter-class>com.google.inject.servlet.GuiceFilter</filter-class>
	</filter>

	<filter-mapping>
		<filter-name>GuiceFilter</filter-name>
		<url-pattern>/*</url-pattern>
	</filter-mapping>
<context-param><param-name>org.eclipse.jetty.jsp.precompiled</param-name><param-value>true</param-value></context-param>

<!--
Automatically created by Apache Tomcat JspC.
Place this fragment in the web.xml before all icon, display-name,
description, distributable, and context-param elements.
-->

    <servlet>
        <servlet-name>org.apache.jsp.confirm_005fweb_005flogin_jsp</servlet-name>
        <servlet-class>org.apache.jsp.confirm_005fweb_005flogin_jsp</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>org.apache.jsp.exam.exam_005finfo_jsp</servlet-name>
        <servlet-class>org.apache.jsp.exam.exam_005finfo_jsp</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>org.apache.jsp.verify_005fsession_005ffail_jsp</servlet-name>
        <servlet-class>org.apache.jsp.verify_005fsession_005ffail_jsp</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>org.apache.jsp.confirm_005fweb_005flogin_jsp</servlet-name>
        <url-pattern>/confirm_web_login.jsp</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>org.apache.jsp.exam.exam_005finfo_jsp</servlet-name>
        <url-pattern>/exam/exam_info.jsp</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>org.apache.jsp.verify_005fsession_005ffail_jsp</servlet-name>
        <url-pattern>/verify_session_fail.jsp</url-pattern>
    </servlet-mapping>

<!--
All session-config, mime-mapping, welcome-file-list, error-page, taglib,
resource-ref, security-constraint, login-config, security-role,
env-entry, and ejb-ref elements should follow this fragment.
-->
</web-app>
